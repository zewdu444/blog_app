
 <%= render 'shared/user', user:@user%>
<div>
 <h3>Bio</h3>
<p><%= @user.bio %><p>
</div>

<div>
<ul>
   <% @user.most_three_recent_post.each do |post| %>
   <li>
      <%= render 'shared/post', post:post %>
